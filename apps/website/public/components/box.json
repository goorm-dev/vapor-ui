        },
        {
            "prop": "foregroundColor",
            "type": ["$primary", "$secondary", "$success", "$warning", "$danger", "$contrast", "$normal"],
            "default": "inherit",
            "description": "요소의 텍스트 색상을 설정합니다. $ 접두어와 함께 사용합니다."
        },
        {
            "prop": "borderRadius",
            "type": ["000", "050", "100", "200", "300", "400", "500", "600", "700", "800", "900"],
            "default": "000",
