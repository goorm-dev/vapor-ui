        {
            "prop": "className",
            "type": ["string"],
            "default": "-",
            "description": "트리거 요소에 적용할 추가 CSS 클래스입니다."
        },
        {
            "prop": "disabled",
            "type": ["boolean"],
            "default": "false",
            "description": "트리거를 비활성화합니다."
