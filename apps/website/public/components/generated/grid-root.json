{
    "name": "GridRoot",
    "displayName": "GridRoot",
    "props": [
        {
            "name": "render",
            "type": ["ReactElement", "(props: HTMLProps) => ReactElement"],
            "required": false,
            "description": "컴포넌트의 HTML 요소를 다른 태그로 대체하거나 다른 컴포넌트와 조합할 수 있습니다.\n\n렌더링할 요소를 반환하는 `ReactElement` 또는 함수를 인수로 받습니다."
        },
        {
            "name": "inline",
            "type": ["boolean"],
            "required": false,
            "description": "인라인 그리드 여부를 결정합니다."
        },
        {
            "name": "templateColumns",
            "type": ["string"],
            "required": false,
            "description": "그리드 열 템플릿을 설정합니다."
        },
        {
            "name": "templateRows",
            "type": ["string"],
            "required": false,
            "description": "그리드 행 템플릿을 설정합니다."
        },
        {
            "name": "flow",
            "type": ["column", "row", "row-dense", "column-dense"],
            "required": false,
            "defaultValue": "row",
            "description": "그리드 아이템 배치 방향을 설정합니다."
        }
    ]
}
