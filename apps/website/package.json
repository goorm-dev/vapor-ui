{
    "name": "website",
    "version": "0.0.8",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "dev": "next dev --turbo",
        "start": "next start",
        "postinstall": "fumadocs-mdx",
        "lint": "eslint ./src",
        "format": "prettier --write \"./src/**/*.{ts,tsx,md}\"",
        "format:check": "prettier --check \"./src/**/*.{ts,tsx,md}\"",
        "docs:generate": "docs-extractor -c ../../packages/core/tsconfig.json -o ./public/components/generated",
        "clean": "rm -rf node_modules .next .turbo .source",
        "typecheck": "tsc --noEmit",
        "docs-api-build": "tsx scripts/docs-api-builder.ts"
    },
    "dependencies": {
        "@base-ui-components/react": "1.0.0-beta.2",
        "@orama/orama": "^3.1.9",
        "@oramacloud/client": "^2.1.4",
        "@radix-ui/react-collapsible": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.2",
        "@radix-ui/react-radio-group": "^1.2.4",
        "@radix-ui/react-tabs": "^1.1.2",
        "@vapor-ui/core": "workspace:*",
        "@vapor-ui/docs-extractor": "workspace:*",
        "@vapor-ui/icons": "workspace:*",
        "clsx": "^2.1.1",
        "fumadocs-core": "^15.3.0",
        "fumadocs-docgen": "^3.0.0",
        "fumadocs-mdx": "^11.6.3",
        "fumadocs-typescript": "^4.0.7",
        "fumadocs-ui": "^15.5.2",
        "lodash-es": "^4.17.21",
        "markdown-table": "^3.0.4",
        "next": "^15.3.5",
        "react": "^18.3.1",
        "react-dom": "^18.1.0",
        "react-error-boundary": "^6.0.0",
        "react-live": "^4.1.8",
        "react-markdown": "^10.1.0",
        "remark": "^15.0.1",
        "remark-gfm": "^4.0.1",
        "remark-mdx": "^3.1.1",
        "sass": "^1.90.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@fumadocs/mdx-remote": "^1.3.4",
        "@repo/eslint-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "@svgr/webpack": "^8.1.0",
        "@tailwindcss/postcss": "^4.1.6",
        "@types/lodash-es": "^4.17.12",
        "@types/mdx": "^2.0.13",
        "@types/node": "^22.15.4",
        "@types/react": "^18.0.1",
        "@types/react-dom": "^18.0.1",
        "eslint": "^9.20.1",
        "globby": "^14.0.0",
        "postcss": "^8.5.4",
        "shiki": "^3.7.0",
        "tailwindcss": "^4.1.6",
        "tsx": "^4.19.2",
        "typescript": "^5.9.2",
        "typescript-api-extractor": "1.0.0-alpha.8"
    }
