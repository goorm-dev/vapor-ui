        "@storybook/builder-vite": "^10.2.10",
        "@storybook/react-vite": "^10.2.10",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vanilla-extract/vite-plugin": "^5.1.4",
