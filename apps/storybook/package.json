        "@storybook/builder-vite": "9.1.17",
        "@storybook/react-vite": "9.1.17",
        "@types/react": "^19.2.3",
        "@types/react-dom": "^19.2.3",
        "@vanilla-extract/vite-plugin": "^5.1.1",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.39.2",
